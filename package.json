		"prestart": "yarn generate-sprite && yarn generate-icon-name-enum",
		"build": "nuxt build",
		"deploy": "firebase deploy --only functions",
		"dev": "nuxt dev",
		"generate": "export NODE_OPTIONS='--max-old-space-size=8192' && nuxt generate",
